{
	"name": "sveltekit-dog-tinder",
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"check": "bun -b svelte-kit sync && bun -b svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"build": "vite build",
		"lint": "eslint --cache .",
		"format": "npm run lint -- --fix",
		"preview": "bun -b vite preview"
	},
	"devDependencies": {
		"@ryoppippi/eslint-config": "^0.3.7",
		"@sveltejs/adapter-static": "^3.0.9",
		"@sveltejs/kit": "^2.27.1",
		"@sveltejs/vite-plugin-svelte": "^4.0.4",
		"@unocss/extractor-svelte": "^66.4.1",
		"daisyui": "^3.9.4",
		"eslint": "^9.32.0",
		"eslint-plugin-format": "^1.0.1",
		"eslint-plugin-svelte": "^3.11.0",
		"std-env": "^3.9.0",
		"svelte": "^5.38.0",
		"svelte-check": "^4.3.1",
		"svelte-meta-tags": "^2.8.0",
		"tslib": "^2.8.1",
		"typescript": "^5.9.2",
		"typescript-svelte-plugin": "^0.3.49",
		"unocss": "^66.4.1",
		"unocss-preset-daisy": "^7.0.0",
		"vite": "^5.4.19"
	},
	"trustedDependencies": [
		"svelte-preprocess"
	]
}
