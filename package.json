{
	"name": "sveltekit-dog-tinder",
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"check": "bun -b svelte-kit sync && bun -b svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"build": "vite build",
		"lint": "eslint --cache .",
		"format": "npm run lint -- --fix",
		"preview": "bun -b vite preview"
	},
	"devDependencies": {
		"@ryoppippi/eslint-config": "npm:@jsr/ryoppippi__eslint-config",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.20.2",
		"@sveltejs/vite-plugin-svelte": "^4.0.4",
		"@unocss/extractor-svelte": "^66.0.0",
		"daisyui": "^3.9.4",
		"eslint": "^9.14.0",
		"eslint-plugin-format": "^1.0.1",
		"eslint-plugin-svelte": "^3.3.3",
		"std-env": "^3.8.1",
		"svelte": "^5.25.0",
		"svelte-check": "^4.1.5",
		"svelte-meta-tags": "^2.8.0",
		"tslib": "^2.8.1",
		"typescript": "^5.8.2",
		"typescript-svelte-plugin": "^0.3.46",
		"unocss": "^66.0.0",
		"unocss-preset-daisy": "^7.0.0",
		"vite": "^5.4.14"
	},
	"trustedDependencies": [
		"svelte-preprocess"
	]
}
