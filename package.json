{
	"name": "sveltekit-dog-tinder",
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"check": "bun -b svelte-kit sync && bun -b svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"build": "vite build",
		"lint": "eslint --cache .",
		"format": "npm run lint -- --fix",
		"preview": "bun -b vite preview"
	},
	"devDependencies": {
		"@ryoppippi/eslint-config": "npm:@jsr/ryoppippi__eslint-config",
		"@sveltejs/adapter-static": "^3.0.6",
		"@sveltejs/kit": "^2.11.1",
		"@sveltejs/vite-plugin-svelte": "^4.0.3",
		"@unocss/extractor-svelte": "^0.65.1",
		"daisyui": "^3.9.4",
		"eslint": "^9.14.0",
		"eslint-plugin-format": "^0.1.3",
		"eslint-plugin-svelte": "^2.46.1",
		"std-env": "^3.8.0",
		"svelte": "^5.13.0",
		"svelte-check": "^4.1.1",
		"svelte-meta-tags": "^2.8.0",
		"tslib": "^2.8.1",
		"typescript": "^5.7.2",
		"typescript-svelte-plugin": "^0.3.44",
		"unocss": "^0.65.1",
		"unocss-preset-daisy": "^7.0.0",
		"vite": "^5.4.11"
	},
	"trustedDependencies": [
		"svelte-preprocess"
	]
}
